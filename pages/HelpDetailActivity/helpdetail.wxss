.ImageLayout{
  display: flex;
  width: 100%;
  height: 400rpx;
  flex-direction: column;
  align-items: center;
}
.LineLayout{
  display: flex;
  width: 100%;
  height: 5rpx;
  background-color: #f5f5f5;
  margin-top: 10rpx;
}
.SingerLayout{
  display: flex;
  width: 100%;
  height: 80rpx;
  flex-direction: row;
  align-items: center;
  padding: 5px 0;
  border-bottom: 1px solid #ededed;
}
.SingerTitleText{
  font-size: 20px;
  color: black;
  font-weight: bold;
  margin-left: 30rpx;
  width: 200rpx;
}
.SingerContextText{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;

  width: 60%;
  font-size: 18px;
  color: gray;
  margin-left: 40rpx;
}
.LogoImage{
  width:  40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.DetailLayout{
  display: flex;
  width: 100%;
  flex-direction: column;
  background-color: #F0F0F0;
}
.DetailTitle{
  font-size: 13px;
  color: black;
  margin-left: 20rpx;
  margin-top: 10rpx;
  font-weight: bold;
}
.DetailContext{
  font-size: 13px;
  color: gray;
  margin-left: 50rpx;
}

.BottomNewServiceLayout{
  display:flex;
  position: fixed;
  left: 0px;
  bottom: 0px;
  z-index: 50;
  width: 100%;
  height: 48px;
  background-color: #E51C23;
  flex-direction:row;
  align-items: center;
  justify-content:center;
}
.NewServiceText{
  font-size: 20px;
  font-weight: bold;
  color: white;
}