.LineLayout{
  display: flex;
  width: 100%;
  height: 10rpx;
  background-color: #F3F5F7;
}
.InputTitleLayout{
  display: flex;
  width: 100%;
  height: 80rpx;
  flex-direction: row;
  align-items: center; 
  padding: 5px 0;
  border-bottom: 1px solid #ededed;
}
.ServiceDetailLayout{
  display: flex;
  width: 100%;
  flex-direction: column;
  padding: 5px 0;
  border-bottom: 1px solid #ededed;
}
.InputTitleText{
  font-size: 16px;
  color: black;
  font-weight:bold;
  margin-left: 50rpx;
  width: 180rpx;
}
.InputEditText{
  display: flex;
  font-size: 16px;
  color: gray;
  margin-left: 10rpx;
}
.InputContextText{
  display: flex;
  height: 100%;
  font-size: 16px;
  color: gray;
  margin-left: 60rpx;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
}
.SingerEndLayout{
  display: flex;
  width: 80%;
  flex-direction: row;
  justify-content: flex-end;          
}
.PlaceText{
  display: flex;
  font-size: 16px;
  color: gray;
  margin-right: 10rpx;
}
.SingerRightImage{
  display: flex;
  width:  40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.PhotoShowLayout{
  display: flex;
  width: 100%;
  height: 200rpx;
  flex-direction: row;
  padding: 5px 0;
  border-bottom: 1px solid #ededed;
  align-items: center;
  margin-left: 40rpx;
}
.TakePhotoImage{
  display: flex;
  width: 70rpx;
  height: 70rpx;
  margin-left: 50rpx;
}
.ShowPhotoImage{
  display: flex;
  width: 20%;
  height: 50%;
  margin: 10rpx;
}